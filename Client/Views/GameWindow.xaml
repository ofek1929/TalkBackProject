<Window x:Class="Client.Views.GameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client.Views"
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
        xmlns:extensions="clr-namespace:Client.Ext"
        mc:Ignorable="d"
        Title="Game Room" Height="450" Width="800"
        DataContext="{Binding  Source={StaticResource Locator}, Path=GameVM}"
        Content="{Binding Grid}">
    
    <Window.Background>
        <ImageBrush ImageSource="/Assets/toctactoe.png"/>
    </Window.Background>


</Window>
